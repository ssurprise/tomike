<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="15dp"
    tools:context="com.skx.tomike.missile.ds.activity.QueueDemoActivity">

    <LinearLayout
        android:id="@+id/tv_queue_sourceWrap"
        android:layout_width="120dp"
        android:layout_height="400dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="80dp"
        android:animateLayoutChanges="true"
        android:background="@color/skx_f5f5f5"
        android:orientation="vertical"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_queue_pushBtn"
        style="@style/TextBtnStyle"
        android:layout_width="80dp"
        android:layout_height="50dp"
        android:layout_marginTop="208dp"
        android:layout_marginEnd="50dp"
        android:text="入队"
        android:textColor="@color/skx_212121"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/tv_queue_popBtn"
        style="@style/TextBtnStyle"
        android:layout_width="80dp"
        android:layout_height="50dp"
        android:text="出队"
        android:textColor="@color/skx_212121"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@id/tv_queue_pushBtn"
        app:layout_constraintTop_toBottomOf="@id/tv_queue_pushBtn"
        app:layout_constraintVertical_bias="0.129"
        tools:ignore="HardcodedText" />

</androidx.constraintlayout.widget.ConstraintLayout>
